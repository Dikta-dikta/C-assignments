#include <stdio.h>
int main() 
{
    char str[100];
    int i = 0;
    printf("Enter a string: ");
    scanf("%s", str);
    while (str[i]) 
    { 
        if (str[i] >= 'A' && str[i] <= 'Z') 
        {
            str[i] = str[i] + 32;
        }
        i++;
    }

    printf("String in lowercase: %s\n", str);
    return 0;
}
